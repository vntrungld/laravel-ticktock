{
    "name": "vntrungld/laravel-ticktock",
    "description": "Laravel TickTock",
    "require": {
	    "php": "^7.2|^8.0",
        "illuminate/support": "^6|^7|^8|^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^7|^8|^9|^10",
        "orchestra/testbench": "^3|^4|^5|^6|^7|^8"
    },
    "license": "MIT",
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Vntrungld\\LaravelTicktock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vntrungld\\LaravelTicktock\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vntrungld\\LaravelTicktock\\LaravelTicktockServiceProvider"
            ],
            "aliases": {
                "Ticktock": "Vntrungld\\LaravelTicktock\\Facades\\Ticktock"
            }
        }
    },
    "authors": [
        {
            "name": "Lam Duc Trung",
            "email": "vn.trungld@gmail.com"
        }
    ]
}
